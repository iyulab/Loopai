{
  "name": "@loopai/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for Loopai - Human-in-the-Loop AI Self-Improvement Framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests examples --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "loopai",
    "ai",
    "machine-learning",
    "loop",
    "validation",
    "program-synthesis",
    "typescript",
    "sdk"
  ],
  "author": "Loopai Team <support@loopai.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iyulab/loopai.git",
    "directory": "sdk/typescript"
  },
  "bugs": {
    "url": "https://github.com/iyulab/loopai/issues"
  },
  "homepage": "https://github.com/iyulab/loopai#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
