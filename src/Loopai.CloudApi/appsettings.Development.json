{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=LoopaiDb_Dev;Trusted_Connection=True;MultipleActiveResultSets=true",
    "Redis": "localhost:6379"
  },
  "CacheSettings": {
    "TaskMetadataTtlMinutes": 5,
    "ActiveArtifactTtlMinutes": 2,
    "ExecutionStatsTtlMinutes": 1,
    "Enabled": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Debug",
        "System": "Information"
      }
    }
  },
  "CodeBeaker": {
    "WebSocketUrl": "ws://localhost:5039/ws/jsonrpc",
    "SessionPoolSize": 10,
    "SessionIdleTimeoutMinutes": 30,
    "SessionMaxLifetimeMinutes": 120,
    "DefaultTimeoutMs": 30000,
    "EnableAutoCleanup": true,
    "CleanupIntervalMinutes": 5,
    "MaxConcurrentExecutionsPerSession": 1,
    "ConnectionTimeoutSeconds": 30,
    "KeepAliveIntervalSeconds": 120,
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000
  },
  "Execution": {
    "Provider": "CodeBeaker"
  }
}
