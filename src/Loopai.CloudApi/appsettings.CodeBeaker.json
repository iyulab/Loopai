{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Loopai.Core.CodeBeaker": "Debug"
    }
  },
  "CodeBeaker": {
    "WebSocketUrl": "ws://localhost:5000/ws/jsonrpc",
    "SessionPoolSize": 10,
    "SessionIdleTimeoutMinutes": 30,
    "SessionMaxLifetimeMinutes": 120,
    "MemoryLimitMB": 512,
    "CpuShares": 1024,
    "DefaultTimeoutMs": 30000,
    "EnableAutoCleanup": true,
    "CleanupIntervalMinutes": 5,
    "MaxConcurrentExecutionsPerSession": 1,
    "ConnectionTimeoutSeconds": 30,
    "KeepAliveIntervalSeconds": 120,
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000,
    "LanguageMapping": {
      "python": "python",
      "javascript": "javascript",
      "typescript": "javascript",
      "go": "go",
      "csharp": "csharp",
      "dotnet": "csharp"
    }
  },
  "Execution": {
    "Provider": "CodeBeaker",
    "Comment": "Set Provider to 'Deno' to use existing edge runtime, 'CodeBeaker' to use CodeBeaker"
  }
}
